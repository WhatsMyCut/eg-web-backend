{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "legacyDecorators": true
      }
    },
    "plugins": ["prettier", "import"],
    "rules": {
      "import/no-unresolved": "warn",
      "no-console": "warn",
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "args": "none",
          "ignoreRestSiblings": true
        }
      ]
    },
    "env": {
      "es6": true,
      "node": true
    },
    "extends": ["eslint:recommended"],
    "globals": {
      "console": true,
      "exports": true,
      "fetch": true,
      "module": true,
      "process": true,
      "require": true,
      "Promise": true
    }
  }
  